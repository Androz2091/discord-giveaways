{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": "eslint:recommended",
    "rules": {
        "indent": ["error", 4],
        "quotes": ["error", "single"],
        "semi": ["error", "always"],
        "eol-last": ["error", "always"],
        "prefer-const": ["warn", "any"],
        "comma-dangle": ["error", "never"],
        "accessor-pairs": "off",
        "no-async-promise-executor": "off",
        "no-unused-vars": "off"
    }
}